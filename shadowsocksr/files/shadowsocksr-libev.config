
config ssr_redir
	option disabled 1
	option server 'ssr0'
	option local_address '0.0.0.0'
	option local_port '1100'
	option mode 'tcp_and_udp'
	option fast_open 1
	option verbose 1
	option reuse_port 1

config ssr_rules 'ssr_rules'
	option disabled 1
	option redir_tcp 'hi'
	option redir_udp 'hi'
	option src_default 'checkdst'
	option dst_default 'bypass'
	option local_default 'checkdst'
	list src_ips_forward '192.168.1.4'
	list dst_ips_forward '8.8.8.8'

config server 'ssr0'
	option disabled 1
	option server '192.168.1.3'
	option server_port '9001'
	option password '********'
	option method 'aes-256-cfb'
	option timeout 300
	option obfs 'plain'
	option obfs_param 'param'
	option protocol 'auth_aes128_md5'
	option protocol_parma 'param'
